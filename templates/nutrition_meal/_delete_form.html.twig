<form method="post" action="{{ path('app_nutrition_meal_delete', {'id': nutrition.id}) }}" onsubmit="return confirm('Voulez-vous supprimer ce repas ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ nutrition.id) }}">
	<button class="btn btn-sm btn-outline-danger"><i class="bi bi-trash-fill me-1"></i>Supprimer</button>
</form>
